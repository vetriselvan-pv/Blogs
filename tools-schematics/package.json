{
  "name": "tools-schematics",
  "version": "0.0.0",
  "description": "A blank schematics",
  "scripts": {
    "build": "tsc -p tsconfig.json && npm run copy:assets",
    "copy:assets": "cp package.json dist/ && sed -i '' 's|\"./src/collection.json\"|\"./collection.json\"|g' dist/package.json && rsync -r --exclude='*.ts' src/ dist/",
    "test": "npm run build && jasmine src/**/*_spec.js"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^20.3.1",
    "@angular-devkit/schematics": "^20.3.1",
    "typescript": "~5.9.2"
  },
  "devDependencies": {
    "@types/node": "^20.17.19",
    "@types/jasmine": "~5.1.0",
    "jasmine": "^5.0.0"
  }
}
